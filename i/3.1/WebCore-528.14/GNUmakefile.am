	if sort CSSValueKeywords.in | uniq -d | grep -E '^[^#]'; then echo 'Duplicate value!'; exit 1; fi
	$(PERL) "$(WebCore)/css/makevalues.pl"
	mv CSSValueKeywords.* $(GENSOURCES)

