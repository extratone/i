  sh-*-* | sh64-*-*)
    case "${host}" in
      i[3456789]86-*-vsta) ;; # don't add gprof back in
      i[3456789]86-*-go32*) ;; # don't add gprof back in
      i[3456789]86-*-msdosdjgpp*) ;; # don't add gprof back in
