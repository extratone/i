package body Ada.Exceptions.Traceback is

   ----------------
   -- Tracebacks --
   ----------------

   function Tracebacks (E : Exception_Occurrence) return Tracebacks_Array is
   begin
      return Tracebacks_Array (E.Tracebacks (1 .. E.Num_Tracebacks));
   end Tracebacks;

end Ada.Exceptions.Traceback;
